{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "outDir": "./lib-ts",
      "lib": ["ESNext", "DOM"],
      "declaration": true,
      "strict": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "./src-ts/**/*"
    ]
}
